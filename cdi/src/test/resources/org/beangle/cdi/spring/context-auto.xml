<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="BeanContainer" class="org.beangle.commons.cdi.spring.context.SpringContainer" >
    <property name="moduleLocations" value="classpath*:META-INF/beangle/cdi.xml" />
    <property name="reconfigLocations" value=";classpath*:spring-config-test.xml"/>
  </bean>

  <bean id="baseTransactionProxy" class="org.beangle.commons.cdi.spring.bean.ProxyFactoryBean"   abstract="true"> </bean>

  <bean id="entityDao" parent="baseTransactionProxy">
    <property name="target">
      <bean class="org.beangle.commons.cdi.spring.bean.TestEntityDao" autowire="byName" />
    </property>
  </bean>
  <import resource="classpath:org/beangle/cdi/spring/context.xml"/>
</beans>
